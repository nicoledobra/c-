#include <iostream>
using namespace std;
int main()
{
    int index, fita;
    cin >> fita;
    index=2;
    while(index <= fita){
    cout << "\n" <<index;
    index=index + 2;
    }
    return 0;
}

#include <iostream>
int main()
{
    int index, fita;
    cin >> fita;
    index=2;
    while(index <= fita){
    cout << index << endl;
    index=index + 2;
    }
    return 0;
}

