#include <iostream>
using namespace std;
int main()
{
    int index;
    index=1;
    while(index <= 10){
    cout << index << endl;
    index=index + 1;
    }
    return 0;
}

#include <iostream>
using namespace std;
int main()
{
    int index;
    index=1;
    while(index <= 10){
    cout << "\n" <<index;
    index=index + 1;
    }
    return 0;
}

